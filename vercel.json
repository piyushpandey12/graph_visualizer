{
  "version": 2,
  "builds": [
    {
      "src": "api/run.py",
      "use": "@vercel/python"
    },
    {
      "src": "templates/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "templates/welcome.html",
      "use": "@vercel/static"
    },
    {
      "src": "static/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/api/run", "dest": "api/run.py" },
    { "src": "/", "dest": "templates/welcome.html" },
    { "src": "/index", "dest": "templates/index.html" },
    { "src": "/static/(.*)", "dest": "static/$1" }
  ]
}
